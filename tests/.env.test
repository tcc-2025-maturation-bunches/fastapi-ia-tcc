# Configurações para ambiente de testes
ENVIRONMENT=test
DEBUG=True

# Configurações AWS (não usadas diretamente nos testes)
AWS_REGION=us-east-1

# DynamoDB
DYNAMODB_TABLE_NAME=fruit-detection-test-results
DYNAMODB_DEVICES_TABLE=fruit-detection-test-devices
DYNAMODB_DEVICE_ACTIVITIES_TABLE=fruit-detection-test-device-activities

# S3
S3_IMAGES_BUCKET=fruit-detection-test-images
S3_RESULTS_BUCKET=fruit-detection-test-results

# EC2 - Configurações do serviço de IA
EC2_IA_ENDPOINT=http://localhost:8001

# Timeout para requisições (menor para testes)
REQUEST_TIMEOUT=2

# Configurações de upload
MAX_UPLOAD_SIZE_MB=5
PRESIGNED_URL_EXPIRY_MINUTES=5

# Configurações de processamento combinado
ENABLE_AUTO_MATURATION=True
MIN_DETECTION_CONFIDENCE=0.6
MIN_MATURATION_CONFIDENCE=0.7

# Configurações de CORS
CORS_ORIGINS=*

# Configurações de logging
LOG_LEVEL=INFO

# Configurações de cache (desativado para testes)
ENABLE_CACHE=False
CACHE_TTL_SECONDS=0
